        "import random\n",
        "\n",
        "def fruits():\n",
        "  fruits_1=input(\"enter the fruits name  \")\n",
        "  options = [\"mangoes\",\"banana\",\"apples\"]\n",
        "  fruits_2= random.choice(options)\n",
        "  choice = {\"fruits_one\" : fruits_1,\"fruits_two\" : fruits_2}\n",
        "  return choice\n",
        "choice = fruits()\n",
        "print(choice)\n"
